{
  "version": 2,
  "builds": [
    {
      "src": "backend/public/index.php",
      "use": "vercel-php@0.6.0"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/public/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "backend/public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "stderr",
    "LOG_LEVEL": "error",
    "CACHE_DRIVER": "array",
    "SESSION_DRIVER": "cookie"
  },
  "functions": {
    "backend/public/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  }
}
